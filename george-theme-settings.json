    "custom_css": "/* BODY FONT */\n\n.post-template-default {\n\tmax-width:700px;\n\tfloat:left;\n}\n\n/* HEADER */\n\n.header {\n\tpadding-bottom:0;\n}\n\n.content-area {\n\tpadding-top:5px;\n}\n\n.header__row {\n\tjustify-content:flex-start;\n}\n\n.header__col-left {\n\tdisplay:none;\n}\n\n.header__menu {\n\tdisplay:block;\n\tpadding:0;\n}\n\n.header__menu li a.nav__link {\n\tmargin:0;\n\tpadding:0;\n}\n\n/* Blog page */\n\n.posts__row {\n\tdisplay: flex;\n  flex-wrap: wrap;\n}\n\n.blog .blog-post-card-3 {\n\t\tflex-direction:column;\n}\n\n.post-item  {\n\twidth:200px!important;\n\tfloat:left;\n\tmargin-right: 1rem;\n}\n\n.post-card__body * {\n\tfont-size:14px!important;\n\tline-height:16px;\npadding:0 0 2px 0!important;\n}\n\n.post-card__image {\n\tmargin:0 0 8px 0!important;\n}\n\n.post-card__heading {\n\tcolor:blue;\n\tfont-style:italic;\n\ttext-decoration:underline;\n}\n\n.post-card__date {\n\tdisplay:none;\n}\n\n/* Single post */\n.single-post .entry-content, .single-portfolio .entry-content\n{\n\tmargin-top:5px;\n}",
