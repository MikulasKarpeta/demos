    "custom_css": "/* site padding */\n  .container, .container-fluid {\n    padding: 1rem!important;\n  }\n\n/* header */\n\n.header {\n\tpadding: 1.5rem 2rem !important;\n}\n\n.single-post .header * {\n\tcolor:black!important;\n}\n\n\n/* portfolio postcard */\n\n.post-card__body {\n\tposition: absolute;\n  bottom: 2rem;\n  left: 2rem;\n\tcolor:white;\n\tz-index:10;\n}\n\n.post-card__heading {\n\tfont-size:22px;\n}\t\n.post-card__image {\n\tmargin:0;\n}\n\n.post-card__overlay {\n\tdisplay:block;\n\topacity:0.3;\n}\n\n.post-card:hover .post-card__overlay{\n  opacity:0;\n\ttransition:all 0.5s;\n}\n\n/* blog post card */\n\n.blog-post-card-3 .post-card__heading {\n\tfont-size:1.5rem;\n}\n\n/* single post, about page */\n\n\t.page-template-default .entry-content, .single-post .entry-content, .page-template-default footer , .single-post footer  {\n\tpadding:0 2rem!important;\n}\n\n.page-template-default .header *, .single-post .header * {\n\tcolor:black!important;\n}\n\n.page-template-default .content-area, .single-post .content-area {\n\tmargin-top:6rem;\n}\n\n/* media */\n\n@media screen and (max-width:600px) {\n\t.header {\n\tpadding: 0.5rem 1rem !important;\n}\n\t\n\t.page-template-default .entry-content, .single-post .entry-content, .page-template-default footer , .single-post footer {\n\tpadding:0 1rem!important;\n}\n\t\n\t.post-card__body {\n  bottom: 1.5rem;\n  left: 1rem;\n}\n\t\n.post-card__heading {\n\tfont-size:16px;\n}\t\n}\n\n\n@media screen and (max-width:781px) {\n\t.footer-widget h5, .footer-widget h4 {\n\ttext-align:center!important;\n}\n}",
