    "custom_css": "/* header */\n.header__brand {\n\tvisibility:hidden;\n}\n\n@media screen and (min-width: 1000px) {\n\t.site .menu-main-container .nav__link {\n\tmargin: 0 3rem!important;\n\t}\n}\n@media screen and (max-width:992px) { \n.site:not(.vertical) .center-menu:not(.center-logo) .header__col-left {\n\tdisplay:none;\n}\n}\n/* menu, portfolio category underline */\n.menu-main-container.underlined .nav__link::after {\n\tborder-bottom-width:2px!important;\n/* \tbottom:10px!important; */\n}\n\n.posts-ajax__tab span.underlined::after {\n\t\tborder-bottom-width:2px!important;\n/* \tbottom:10px!important; */\n}\n\n/* portfolio page */\n\n.posts-ajax__categories-wrapper {\n\tmargin: 0 auto 2rem auto!important;\n\tjustify-content:center;\n}\n\n@media screen and (min-width:1000px) {.posts-ajax__categories-wrapper label{\n\tmargin: 0 2rem;\n\t}}\n\n.post-card__heading {\n\tfont-size:32px;\n}\n\n@media screen and (max-width:600px) { \n\t.post-card__heading {\n\tfont-size:24px;\n}\n}\n\n@media screen and (max-width:420px) { \n\t.post-card__heading {\n\tfont-size:18px;\n}\n}\n\n/* single portfolio bg */\n.single-portfolio {\n\tbackground-color:#b56769;\n}\n\n.single-portfolio .entry-title {\nmargin-bottom:4rem;\n}",
